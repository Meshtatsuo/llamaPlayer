          <div class="row">
            <!--Row with artist/album/song-->
            <div id="artist-col" class="block-item col s12 m4">
              <div class="block-item card-bg overflow">
                <ul class="collection with-header">
                  <li class="collection-header"><h5>ARTISTS</h5></li>
                    {{#each artists as |artist|}}
                        {{> artists artist}}
                    {{/each}}
                </ul>
              </div>
            </div>
            <div id="album-col" class="block-item col s12 m4">
              <div class="block-item card-bg overflow">
                <ul class="collection with-header">
                  <li class="collection-header"><h5>ALBUMS</h5></li>
                    {{#each albums as |album|}}
                        {{> albums album}}
                    {{/each}}
                </ul>
              </div>
            </div>
            <div id="song-col" class="block-item col s12 m4">
              <div class="block-item card-bg overflow">
                <ul class="collection with-header">
                  <li class="collection-header"><h5>TRACKS</h5></li>
                    {{#each tracks as |track|}}
                        {{> tracks track}}
                    {{/each}}
                </ul>
              </div>
            </div>

          </div>
          <div class="row">
            <div class="col s12">
              <div id="queue" class="now-playing card-bg">
                <div class="row valign-wrapper">
                  <!-- Album Art -->
                  <div class="col s12 m3">
                    <img
                      src="./images/placeholder_albumart.png"
                      alt="placeholder album art"
                    />
                  </div>

                  <!-- Now Playing -->
                  <div class="col s12 m4 left-align">
                    <h4>Now Playing:</h4>
                    <h4>Feed the Wolves</h4>
                    <H6>Breaking Benjamin</H6>
                    <h6>Ember</h6>
                  </div>

                  <!--Queue-->
                  <div class="col s12 m4">
                    <h4>Up Next:</h4>
                    <div id="playlist" class="">
                      <ul class="collection">
                        <li class="collection-item">Ember</li>
                        <li class="collection-item">Galaxy 2 Galaxy</li>
                        <li class="collection-item">Ziltoid</li>
                        <li class="collection-item">Empath</li>
                        <li class="collection-item">All Things Must Pass</li>
                        <li class="collection-item">Kid A</li>
                        <li class="collection-item">House Rhythms</li>
                        <li class="collection-item">Ok Computer</li>
                        <li class="collection-item">2018</li>
                        <li class="collection-item">Fast Times at Ridgemont High</li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>